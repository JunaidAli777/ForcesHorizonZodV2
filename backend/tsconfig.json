{
    "compilerOptions": {
      "target": "ESNext",
      "module": "CommonJS",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "types": ["node"],
      "jsx": "react-jsx"
    },
    "include": ["*.ts"],
    "exclude": ["node_modules"]
}